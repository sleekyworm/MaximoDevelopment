            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

var newDate = new Date();
//DEBUGGING code, sets date for testing
//newDate= new Date(2004,0,1);

newDate= DateTimeSpan.addDate(newDate,0,0,reportContext.getParameterValue("no_days"));

// Add query to sqlText variable.
sqlText = "select distinct workorder.wonum, workorder.schedstart, workorder.schedfinish, "
+ "workorder.description wodesc, wpitem.location, wpitem.storelocsite as wpsite, "
+ "workorder.siteid as wosite, workorder.woclass, wpitem.itemnum, wpitem.description itemdesc, "
+ "wpitem.conditioncode, wpitem.itemqty "
+ "from invreserve,  workorder,  wpitem "
+ "where invreserve.wonum  =  workorder.wonum "
+ "and invreserve.siteid  =  workorder.siteid "
+ "and invreserve.itemnum = wpitem.itemnum "
+ "and invreserve.location = wpitem.location "
+ "and invreserve.wonum = wpitem.wonum "
+ "and "
+ maximoDataSet.getNullValueFunction("invreserve.conditioncode", "'1'") + "="
+ maximoDataSet.getNullValueFunction("wpitem.conditioncode", "'1'")
+ " and wpitem.linetype = "
+ "(select value from synonymdomain where domainid = 'LINETYPE' and maxvalue = 'ITEM') "
+ "and wpitem.itemqty > 0 "
+ "and ((invreserve.requireddate <= " + MXReportSqlFormat.getTimestampFunction(newDate) + " ) or "
+ "((workorder.schedstart <= " + MXReportSqlFormat.getTimestampFunction(newDate)
+ " and invreserve.requireddate is null)" 
+ "or (workorder.status in "
+ "(select value from synonymdomain where domainid = 'WOSTATUS' and maxvalue = 'APPR') "
+ "and workorder.schedstart is null))) "
// Include the Maximo where clause
+ "and " + params["where"]
+ " order by schedstart, schedfinish, wonum, wodesc, location, wpsite, wosite, "
+ " woclass, itemnum, itemdesc, conditioncode, itemqty"
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[do {

if (!maximoDataSet.fetch())
	return (false);

var newDate2 = new Date();
newDate2= DateTimeSpan.addDate(newDate2,0,0,reportContext.getParameterValue("no_days"));

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["wonum"] = maximoDataSet.getString("wonum");
row["wodesc"] = maximoDataSet.getString("wodesc");
row["location"] = maximoDataSet.getString("location");
row["wpsite"] = maximoDataSet.getString("wpsite");
row["wosite"] = maximoDataSet.getString("wosite");
row["woclass"] = maximoDataSet.getString("woclass");
row["itemnum"] = maximoDataSet.getString("itemnum");
row["itemdesc"] = maximoDataSet.getString("itemdesc");	
row["conditioncode"] = maximoDataSet.getString("conditioncode");
row["schedstart"] = maximoDataSet.getTimestamp("schedstart");
row["schedfinish"] = maximoDataSet.getTimestamp("schedfinish");		
row["itemqty"] = maximoDataSet.getDouble("itemqty");


// Current Balance 
	curBalDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "curBalDataSet");
	curBalDataSet.open();
	
	curBalSQL = "select sum(invbalances.curbal) curbal from invbalances where invbalances.itemnum = '"
	+ row["itemnum"] + "' and invbalances.location = '" + row["location"] + "' and invbalances.siteid = '" 
	+ row["wpsite"] + "'" 
	;
	
	curBalDataSet.setQuery(curBalSQL);

	if(curBalDataSet.fetch()) {
		row["curbal"] = curBalDataSet.getString("curbal");
	}
	
	curBalDataSet.close();
// End Current Balance Description
 
// Exp Issues
	expIssuesDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "expIssuesDataSet");
	expIssuesDataSet.open();
	
	var addOnSQL = "";
	
	if (row["conditioncode"] == null) {
		addOnSQL = "'1'";
	} else {
		addOnSQL = row["conditioncode"];
	}
	expIssuesSQL = "select sum(reservedqty) exp_issues from invreserve, workorder, wpitem where "
	+ " invreserve.itemnum = '" + row["itemnum"] + "' "
    + " and invreserve.location = '" + row["location"] 
    + "' and invreserve.siteid = '" + row["wpsite"] + "' " 
    + " and invreserve.wonum = '" + row["wonum"] +  "' " 
	+ " and  invreserve.itemnum = wpitem.itemnum "
	+ " and invreserve.itemsetid = wpitem.itemsetid "  
	+ " and invreserve.requestnum = wpitem.requestnum " 
	+ " and invreserve.wonum = workorder.wonum "
	+ " and invreserve.siteid = workorder.siteid "
    + " and ((invreserve.requireddate <= " + MXReportSqlFormat.getTimestampFunction(newDate2) + ") "
    + " or ((workorder.schedstart <= " + MXReportSqlFormat.getTimestampFunction(newDate2) 
    + " and invreserve.requireddate is null) "
    + " or (workorder.status in "
    + " (select value from synonymdomain where domainid = 'WOSTATUS' and maxvalue = 'APPR' ) "
    + " and workorder.schedstart is null)))" 
	;
	expIssuesDataSet.setQuery(expIssuesSQL);

	row["exp_issue"]=0;
	if(expIssuesDataSet.fetch()) {
		row["exp_issue"] = expIssuesDataSet.getString("exp_issues");
		if(BirtComp.equalTo(row["exp_issue"],null)){ 
			row["exp_issue"]=0;
		}
	}
	
	expIssuesDataSet.close();
// End Exp Issues Description

// Exp Expires
	expExpiresDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "expExpiresDataSet");
	expExpiresDataSet.open();
	
	expExpiresSQL = "select sum(invbalances.curbal) exp_expir from invbalances, invlot "
	+ "where invbalances.itemnum = '" + row["itemnum"] + "' "
	+ " and invbalances.location = '" + row["location"] 
	+ "' and invbalances.siteid = '" + row["wpsite"] + "' " 
	+ " and invbalances.siteid = invlot.siteid "
	+ " and invbalances.lotnum = invlot.lotnum " 
	+ " and invbalances.itemnum = invlot.itemnum "
	+ " and invbalances.itemsetid = invlot.itemsetid "
	+ " and invbalances.location = invlot.location "
	+ " and " + MXReportSqlFormat.getTimestampFunction(newDate2) 
	+ " >= invlot.useby " 
	;
	
	expExpiresDataSet.setQuery(expExpiresSQL);

	row["exp_expire"]=0;
	if(expExpiresDataSet.fetch()) {
		row["exp_expire"] = expExpiresDataSet.getString("exp_expir");
		if(BirtComp.equalTo(row["exp_expire"],null)){ 
			row["exp_expire"]=0;
		}
	}
	
	expExpiresDataSet.close();
// End Exp Expires Description

// Exp Receipts 
	expReceiptsDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "expReceiptsDataSet");
	expReceiptsDataSet.open();
	
	expReceiptsSQL = "select ((" + expReceiptsDataSet.getNullValueFunction("poline.orderqty", "0") + " - " 
	+ expReceiptsDataSet.getNullValueFunction("poline.receivedqty", "0") + " - " 
	+ expReceiptsDataSet.getNullValueFunction("poline.rejectedqty", "0") + " * " 
	+ expReceiptsDataSet.getNullValueFunction("poline.conversion", "0") + ")) exp_rec "
	+ " from po, poline "
	+ " where poline.ponum = po.ponum "
	+ " and poline.revisionnum = po.revisionnum "
	+ " and  poline.siteid = po.siteid "
	+ " and poline.itemnum = '" + row["itemnum"] + "' "
	+ " and poline.storeloc = '" + row["location"] + "' and poline.siteid = '" + row["wpsite"] + "' "  
	+ " and poline.receiptscomplete = 0 "
	+ " and po.status in (select value from synonymdomain where domainid = 'POSTATUS' and maxvalue = 'APPR')"
	;

	expReceiptsDataSet.setQuery(expReceiptsSQL);

	row["exp_rec"]=0;
	if(expReceiptsDataSet.fetch()) {
		row["exp_rec"] = expReceiptsDataSet.getString("exp_rec");
		if(BirtComp.equalTo(row["exp_rec"],null)){ 
			row["exp_rec"]=0;
		}
	}
	
	expReceiptsDataSet.close();
// End Exp Receipts Description
row["exp_bal"] = row["curbal"] + row["exp_rec"] - (row["exp_issue"] + row["exp_expire"]);

} while (BirtComp.greaterThan(row["exp_bal"],row["itemqty"]));

return (true);]]></method>
